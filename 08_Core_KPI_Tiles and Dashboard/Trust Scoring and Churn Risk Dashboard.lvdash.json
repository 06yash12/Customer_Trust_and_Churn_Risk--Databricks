{
  "datasets": [
    {
      "name": "018239cd",
      "displayName": "Total Customers",
      "queryLines": [
        "SELECT COUNT(*) AS total_customers\r\n",
        "FROM ai_trust_score_project.trust_predictions_final;"
      ]
    },
    {
      "name": "8cab485d",
      "displayName": "High-Risk Customers",
      "queryLines": [
        "SELECT COUNT(*) AS high_risk_customers\r\n",
        "FROM ai_trust_score_project.trust_predictions_final\r\n",
        "WHERE final_decision = 'HIGH_RISK';"
      ]
    },
    {
      "name": "325ef0e3",
      "displayName": "High-Risk Percentage",
      "queryLines": [
        "SELECT \r\n",
        "  ROUND(\r\n",
        "    100.0 * SUM(CASE WHEN final_decision = 'HIGH_RISK' THEN 1 ELSE 0 END) / COUNT(*),\r\n",
        "    2\r\n",
        "  ) AS high_risk_percentage\r\n",
        "FROM ai_trust_score_project.trust_predictions_final;"
      ]
    },
    {
      "name": "eae0cff1",
      "displayName": "Risk Classification Split",
      "queryLines": [
        "SELECT final_decision, COUNT(*) AS customers\r\n",
        "FROM ai_trust_score_project.trust_predictions_final\r\n",
        "GROUP BY final_decision;"
      ]
    },
    {
      "name": "951e3168",
      "displayName": "Confidence Distribution",
      "queryLines": [
        "SELECT\r\n",
        "  CASE\r\n",
        "    WHEN prediction_confidence >= 0.9 THEN '0.9 – 1.0'\r\n",
        "    WHEN prediction_confidence >= 0.7 THEN '0.7 – 0.9'\r\n",
        "    WHEN prediction_confidence >= 0.5 THEN '0.5 – 0.7'\r\n",
        "    ELSE '< 0.5'\r\n",
        "  END AS confidence_bucket,\r\n",
        "  COUNT(*) AS customers\r\n",
        "FROM ai_trust_score_project.trust_predictions_final\r\n",
        "GROUP BY confidence_bucket\r\n",
        "ORDER BY confidence_bucket DESC;"
      ]
    },
    {
      "name": "7097739b",
      "displayName": "Risk by Subscription Type",
      "queryLines": [
        "SELECT \r\n",
        "  c.subscription_type,\r\n",
        "  ROUND(AVG(p.prediction_confidence), 3) AS avg_risk\r\n",
        "FROM ai_trust_catalog.churn_trust.silver_customer_churn c\r\n",
        "JOIN ai_trust_score_project.trust_predictions_final p\r\n",
        "ON c.customerid = p.customerid\r\n",
        "GROUP BY c.subscription_type\r\n",
        "ORDER BY avg_risk DESC;"
      ]
    },
    {
      "name": "034a9b32",
      "displayName": "Risk by Age Group",
      "queryLines": [
        "SELECT\r\n",
        "  CASE\r\n",
        "    WHEN age < 25 THEN 'Under 25'\r\n",
        "    WHEN age BETWEEN 25 AND 40 THEN '25–40'\r\n",
        "    WHEN age BETWEEN 41 AND 60 THEN '41–60'\r\n",
        "    ELSE '60+'\r\n",
        "  END AS age_group,\r\n",
        "  ROUND(AVG(p.prediction_confidence), 3) AS avg_risk\r\n",
        "FROM ai_trust_catalog.churn_trust.silver_customer_churn c\r\n",
        "JOIN ai_trust_score_project.trust_predictions_final p\r\n",
        "ON c.customerid = p.customerid\r\n",
        "GROUP BY age_group\r\n",
        "ORDER BY avg_risk DESC;\r\n"
      ]
    },
    {
      "name": "44ef307d",
      "displayName": "Risk vs Support Calls",
      "queryLines": [
        "SELECT\r\n",
        "  support_calls,\r\n",
        "  ROUND(AVG(p.prediction_confidence), 3) AS avg_risk\r\n",
        "FROM ai_trust_catalog.churn_trust.silver_customer_churn c\r\n",
        "JOIN ai_trust_score_project.trust_predictions_final p\r\n",
        "ON c.customerid = p.customerid\r\n",
        "GROUP BY support_calls\r\n",
        "ORDER BY support_calls;\r\n"
      ]
    },
    {
      "name": "822dc16d",
      "displayName": "Risk vs Payment Delay",
      "queryLines": [
        "SELECT\r\n",
        "  payment_delay,\r\n",
        "  ROUND(AVG(p.prediction_confidence), 3) AS avg_risk\r\n",
        "FROM ai_trust_catalog.churn_trust.silver_customer_churn c\r\n",
        "JOIN ai_trust_score_project.trust_predictions_final p\r\n",
        "ON c.customerid = p.customerid\r\n",
        "GROUP BY payment_delay\r\n",
        "ORDER BY payment_delay;"
      ]
    },
    {
      "name": "4418b20c",
      "displayName": "Top 20 High-Risk Customers",
      "queryLines": [
        "SELECT\r\n",
        "  c.customerid,\r\n",
        "  c.subscription_type,\r\n",
        "  c.total_spend,\r\n",
        "  p.prediction_confidence\r\n",
        "FROM ai_trust_catalog.churn_trust.silver_customer_churn c\r\n",
        "JOIN ai_trust_score_project.trust_predictions_final p\r\n",
        "ON c.customerid = p.customerid\r\n",
        "WHERE p.final_decision = 'HIGH_RISK'\r\n",
        "ORDER BY p.prediction_confidence DESC\r\n",
        "LIMIT 20;\r\n"
      ]
    },
    {
      "name": "8723c99a",
      "displayName": "Average Confidence",
      "queryLines": [
        "SELECT ROUND(AVG(prediction_confidence), 3) AS avg_confidence\r\n",
        "FROM ai_trust_score_project.trust_predictions_final;\r\n"
      ]
    },
    {
      "name": "c35bdb4a",
      "displayName": "Confidence Stability",
      "queryLines": [
        "SELECT ROUND(STDDEV(prediction_confidence), 3) AS confidence_std_dev\r\n",
        "FROM ai_trust_score_project.trust_predictions_final;\r\n"
      ]
    },
    {
      "name": "8d81e138",
      "displayName": "HIGH-RISK Customers Card",
      "queryLines": [
        "SELECT \r\n",
        "  COUNT(*) AS high_risk_customers\r\n",
        "FROM ai_trust_score_project.trust_predictions_final\r\n",
        "WHERE final_decision = 'HIGH_RISK';\r\n"
      ]
    },
    {
      "name": "a2868bb2",
      "displayName": "LOW-RISK Customers Card",
      "queryLines": [
        "SELECT \r\n",
        "  COUNT(*) AS low_risk_customers\r\n",
        "FROM ai_trust_score_project.trust_predictions_final\r\n",
        "WHERE final_decision = 'LOW_RISK';\r\n"
      ]
    },
    {
      "name": "1743efdd",
      "displayName": "Customer Counts by Prediction Confidence Range",
      "queryLines": [
        "SELECT\r\n",
        "  CASE\r\n",
        "    WHEN prediction_confidence < 0.2 THEN '< 0.2'\r\n",
        "    WHEN prediction_confidence < 0.3 THEN '0.2 – 0.3'\r\n",
        "    WHEN prediction_confidence < 0.4 THEN '0.3 – 0.4'\r\n",
        "    WHEN prediction_confidence < 0.5 THEN '0.4 – 0.5'\r\n",
        "    WHEN prediction_confidence < 0.6 THEN '0.5 – 0.6'\r\n",
        "    WHEN prediction_confidence < 0.7 THEN '0.6 – 0.7'\r\n",
        "    WHEN prediction_confidence < 0.8 THEN '0.7 – 0.8'\r\n",
        "    WHEN prediction_confidence < 0.9 THEN '0.8 – 0.9'\r\n",
        "    ELSE '0.9 – 1.0'\r\n",
        "  END AS confidence_bucket,\r\n",
        "  COUNT(*) AS customers\r\n",
        "FROM ai_trust_score_project.trust_predictions_final\r\n",
        "GROUP BY confidence_bucket\r\n",
        "ORDER BY\r\n",
        "  CASE confidence_bucket\r\n",
        "    WHEN '< 0.2' THEN 1\r\n",
        "    WHEN '0.2 – 0.3' THEN 2\r\n",
        "    WHEN '0.3 – 0.4' THEN 3\r\n",
        "    WHEN '0.4 – 0.5' THEN 4\r\n",
        "    WHEN '0.5 – 0.6' THEN 5\r\n",
        "    WHEN '0.6 – 0.7' THEN 6\r\n",
        "    WHEN '0.7 – 0.8' THEN 7\r\n",
        "    WHEN '0.8 – 0.9' THEN 8\r\n",
        "    ELSE 9\r\n",
        "  END;\r\n"
      ]
    }
  ],
